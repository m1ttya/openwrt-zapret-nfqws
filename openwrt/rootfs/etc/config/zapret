config zapret 'config'
	option enabled '1'
	option if_lan 'auto'
	option if_wan 'auto'
	option queue_num '100'
	option ports_tcp '443'
	option ports_udp '443'
	option mode 'auto'
	option profile 'general'
	option hostlist '/etc/zapret/lists/list-general.txt'
	option ipset_enable '1'
	option ipset_path '/etc/zapret/lists/ipset-all.txt'
	option extra_opts ''
